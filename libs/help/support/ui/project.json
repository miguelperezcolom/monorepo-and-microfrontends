{
  "projectType": "library",
  "sourceRoot": "libs/help/support/ui/src",
  "prefix": "monorepo",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/help/support/ui"],
      "options": {
        "jestConfig": "libs/help/support/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/help/support/ui/**/*.ts",
          "libs/help/support/ui/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/help/support/ui/.storybook",
        "browserTarget": "help-support-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/help-support-ui",
        "configDir": "libs/help/support/ui/.storybook",
        "browserTarget": "help-support-ui:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": []
}
